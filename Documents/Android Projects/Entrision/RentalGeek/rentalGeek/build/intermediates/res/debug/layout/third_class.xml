<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:src="@drawable/third" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="@dimen/loho_wid"
        android:layout_height="@dimen/loho_hei"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="36dp"
        android:src="@drawable/logo" />

    <ImageView
        android:id="@+id/tm"
        android:layout_width="300dip"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView1"
        android:layout_centerHorizontal="true"
        android:src="@drawable/geeklink" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/loho_wid"
        android:layout_height="@dimen/loho_hei"
        android:layout_below="@+id/tm"
        android:layout_centerHorizontal="true"
        android:src="@drawable/approved" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="30dip"
        android:layout_marginRight="30dp"
        android:layout_marginTop="33dp"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/armed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/armed"
            android:textColor="@android:color/white"
            android:textSize="16sp"
            android:textStyle="bold" />
    </LinearLayout>

</RelativeLayout><!-- From: file:/Users/travis/Documents/Android%20Projects/Entrision/RentalGeek/rentalGeek/src/main/res/layout/third_class.xml -->