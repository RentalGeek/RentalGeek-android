<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="MagicTextView">
		<attr name="innerShadowColor" format="color"/>
		<attr name="innerShadowRadius" format="dimension"/>
		<attr name="innerShadowDx" format="dimension"/>
		<attr name="innerShadowDy" format="dimension"/>
		
		<attr name="outerShadowColor" format="color"/>
		<attr name="outerShadowRadius" format="dimension"/>
		<attr name="outerShadowDx" format="dimension"/>
		<attr name="outerShadowDy" format="dimension"/>
		
		<attr name="typeface" format="string" />
		
		<attr name="foreground" format="reference|color"/>
		<attr name="backgrounds" format="reference|color"/>
		
		<attr name="strokeWidths" format="dimension" />
		<attr name="strokeMiter" format="dimension" />
		<attr name="strokeColors" format="color" />
		<attr name="strokeJoinStyle">
		    <enum name="miter" value="0" />
		    <enum name="bevel" value="1" />
		    <enum name="round" value="2" />
		</attr>
	</declare-styleable>

	 <declare-styleable name="RangeSeekBar">
        <attr name="absoluteMinValue" format="integer|float"/>
        <attr name="absoluteMaxValue" format="integer|float"/>
        <attr name="singleThumb" format="boolean"/>
    </declare-styleable>
    
    <declare-styleable name="SearchOptionButton">
        <attr name="value" format="string"/>
        <attr name="unselected_bg" format="reference"/>
        <attr name="selected_bg" format="reference"/>
        <attr name="selected_txt_color" format="reference"/>
        <attr name="unselected_txt_color" format="reference"/>
    </declare-styleable>
</resources>
